  },
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.7.0+sha256.b35018fbfa8f583668b2649e407922a721355cd81f61beeb4ac1d4258e585559",
  "dependencies": {
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "globals": "^15.11.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
